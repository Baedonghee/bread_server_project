{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "빵순이빵돌이 서버",
    "description": "빵순이빵돌이 서버 api 문서 입니다.",
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "basePath": "/",
  "tags": [
    {
      "name": "admin",
      "description": "관리자 회원 정보입니다."
    }
  ],
  "schemes": ["http"],
  "paths": {
    "/admin/signin": {
      "post": {
        "tags": ["admin"],
        "summary": "관리자 로그인",
        "operationId": "addAdminSignin",
        "requestBody": {
          "description": "관리자 로그인 파라미터",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AdminSignin"
              }
            }
          },
          "required": true
        },
        "responses": {
          "400": {
            "description": "Invalid input"
          }
        },
        "x-codegen-request-body-name": "body"
      }
    },
    "/admin/signup": {
      "post": {
        "tags": ["admin"],
        "summary": "관리자 회원가입",
        "operationId": "addAdminSignup",
        "requestBody": {
          "description": "관리자 회원가입 파라미터",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AdminSignup"
              }
            }
          },
          "required": true
        },
        "responses": {
          "400": {
            "description": "Invalid input"
          }
        },
        "x-codegen-request-body-name": "body"
      }
    },
    "/admin/current": {
      "get": {
        "tags": ["admin"],
        "summary": "관리자 회원 정보",
        "operationId": "addAdminCurrent",
        "responses": {
          "403": {
            "description": "forbidden"
          }
        },
        "security": [
          {
            "authorization": []
          }
        ],
        "x-codegen-request-body-name": "body"
      }
    }
  },
  "components": {
    "schemas": {
      "AdminSignin": {
        "type": "object",
        "required": ["email", "password"],
        "properties": {
          "email": {
            "type": "string",
            "example": "dongdong@naver.com"
          },
          "password": {
            "type": "string",
            "example": "qwe1234!"
          }
        }
      },
      "AdminSignup": {
        "type": "object",
        "required": ["email", "password", "name", "type"],
        "properties": {
          "email": {
            "type": "string",
            "example": "dongdong@naver.com"
          },
          "password": {
            "type": "string",
            "example": "qwe1234!"
          },
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "description": "가입 종류",
            "enum": ["bread", "google", "facebook", "kakao"]
          }
        }
      }
    },
    "securitySchemes": {
      "authorization": {
        "type": "apiKey",
        "name": "authorization",
        "in": "header"
      }
    }
  }
}
